@article{Mitchell:1967a,
	Author = {Mitchell, Peter and Moyle, Jennifer},
	Date-Added = {2019-10-13 18:53:15 +0100},
	Date-Modified = {2019-10-13 18:53:27 +0100},
	Doi = {10.1042/bj1051147},
